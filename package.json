    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/username/resilient-email-service.git"
  },
  "bugs": {
    "url": "https://github.com/username/resilient-email-service/issues"
  },
  "homepage": "https://github.com/username/resilient-email-service#readme",

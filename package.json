{
  "name": "resilient-email-service",
  "version": "1.0.0",
  "description": "A resilient email sending service with retry logic, fallback, and rate limiting",
  "main": "src/index.js",
  "type": "module",
  "bin": {
    "email-service": "./cli/index.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "node scripts/test.js",
    "test:node": "node --test tests/**/*.test.js",
    "test:watch": "node --test --watch tests/**/*.test.js",
    "dev": "node --watch src/index.js",
    "examples": "node examples/comprehensive.js",
    "cli": "node cli/index.js",
    "cli:help": "node cli/index.js help",
    "cli:test": "node cli/index.js test",
    "cli:status": "node cli/index.js status"
  },
  "keywords": [
    "email",
